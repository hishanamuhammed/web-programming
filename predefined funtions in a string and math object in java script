<script>
var str = "hishana muhammed";
document.write("Character at 5 is: " + str.charAt(5));
document.write("<br>Index of <q>muhammed</q>: " + str.indexOf("muhammed"));
var lw = "hishana muhammed";
document.write("<br>To uppercase: " + str.toUpperCase(lw));
document.write("<br>")
var num = Math.abs(-10)
document.write("Absolute value: ", +num);
var c = 57.89
var x = Math.ceil(c)
document.write("<br>ceil value of ",c," is: " + x);
var power = Math.pow(4, 5);
document.write("<br> Power of the given number is: " + power);
</script>
